ation: 100ms;\n        animation-direction: reverse;\n\n    }\n\n    @keyframes annimation_barre_hrizontal {\n         0% {\n             opacity: 0;\n             border:0px solid transparent;\n             transform: translateY(153px);\n         }\n        100% {\n            opacity: 1;\n            transform: translateY(0px);\n        }\n    }\n\n\n\n    .croix_tags_features {\n        position: absolute;\n        top: 10vh;\n        left: 10vw;\n    }\n\n\n\n\n\n\n    /* iphone X */\n    @media only screen and (max-width: 375px)   and (max-height: 812px) {\n\n        .conteneur_second_block {\n            .Mockup-feature {\n                animation: none;\n                margin: auto;\n                transform: scale(0.6) translateY(-180px);\n            }\n            .conteneur_feature {\n                width: 120%;\n                transform: scale(0.7) translate(-100px , 230px);\n            }\n\n        }\n    }\n\n    /* iphone 6/7 */\n    @media only screen and (max-width: 414px)   and (max-height: 736px) {\n\n        .conteneur_second_block {\n            .Mockup-feature {\n                animation: none;\n                margin: auto;\n                transform: scale(0.5) translateY(-250px);\n            }\n            .conteneur_feature {\n                width: 140%;\n                transform: scale(0.6) translate(-200px , 180px);\n            }\n\n        }\n    }\n\n    /* iphone 5 / SE */\n    @media only screen and (max-width: 320px)   and (max-height: 568px) {\n\n        .conteneur_second_block {\n            .Mockup-feature {\n                animation: none;\n                margin: auto;\n                transform: scale(0.4) translateY(-400px);\n            }\n            .conteneur_feature {\n                width: 170%;\n                transform: scale(0.5) translate(-270px , 30px);\n            }\n\n        }\n    }\n\n    /* ipad PRO */\n    @media only screen and  (max-width: 1025px)   and (max-height: 1366px)  and (min-width: 1